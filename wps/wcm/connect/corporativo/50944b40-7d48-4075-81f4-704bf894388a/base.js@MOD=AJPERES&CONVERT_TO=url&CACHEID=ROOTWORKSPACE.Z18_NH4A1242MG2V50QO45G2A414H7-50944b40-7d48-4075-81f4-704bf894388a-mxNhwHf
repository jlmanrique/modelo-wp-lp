function renderChat($target){'use strict';var $trigger=$target.find('.lq-chat-call'),$chatbox=$target.find('.lq-chat-session'),$chatdiv=$target.find('.lq-chat-content'),$btcancel=$target.find('.lq-chat-options__cancel'),$btaccept=$target.find('.lq-chat-options__accept'),$bttrigger=$target.find('.lq-chat-call__button'),$templateUser=$target.find('.lq-chat-template .lq-chat-user'),$templatePlan=$target.find('.lq-chat-template .lq-chat-plan'),$templatePost=$target.find('.lq-chat-template .lq-chat-post'),$templateClient=$target.find('.lq-chat-template .lq-chat-client'),$templateOptions=$target.find('.lq-chat-template .lq-chat-options'),pathajax=$target.attr('data-json'),interval=0,odata={},oitem={},actualItem="init01",acceptItem="",cancelItem="";function num2digit(d){return(d<10)?'0'+d.toString():d.toString();}
function getTime(){var date=new Date(),hour=date.getHours(),minutes=date.getMinutes(),ampm="AM";if(hour>12){hour=hour-12;ampm="PM";}
return num2digit(hour)+":"+num2digit(minutes)+" "+ampm;}
function onChatScroll(){clearInterval(interval);interval=setTimeout(checkScroll,100);}
function checkScroll(){if(isActiveChat){var triggeroffset=$bttrigger.offset().top,triggerspacer=60+160,headerheight=$('.lq-header').height(),chatheight=200,windowoffset=$(window).scrollTop();if(windowoffset>(triggeroffset-triggerspacer-headerheight)&&windowoffset<(triggeroffset-triggerspacer-headerheight+chatheight)){$(window).off('scroll',onChatScroll);showChatBox();}}}
