(function(){"use strict";var home,typing,homehero;home={sliders:$('[data-slick]'),topPosition:0,profileName:$('.lq-coachs .lq-profile .lq-profile__name'),profilePosition:$('.lq-coachs .lq-profile .profile__position'),profileBtn:$('.lq-coachs .lq-profile .lq-profile__btn'),resetBullet:function(){setTimeout(function(){home.sliders.each(function(_i,_slide){_i=_i+1;$(_slide).find('.slick-dots li').each(function(i,bullet){$(bullet).find('button').html('<span>'+(i+1)+'</span>'+' DE '+$(_slide).find('.slick-dots li').length);});});},200);},resizeEvent:function(){$(window).on('resize',function(){home.resetBullet();home.updateImage();home.profileName.removeAttr('style');home.profilePosition.removeAttr('style');home.profileBtn.removeAttr('style');home.setHeightCoach();});},setHeightCoach:function(){var height_name=0,height_position=0,height_btn=0;home.profileName.each(function(i,item_name){if($(item_name).height()>height_name){home.profileName.css('height',$(item_name).height());}});home.profilePosition.each(function(i,item_position){if($(item_position).height()>height_position){home.profilePosition.css('height',$(item_position).height());}});home.profileBtn.each(function(i,item_btn){if($(item_btn).height()>height_btn){home.profileBtn.css('height',$(item_btn).height());}});},slick:function(){$('[data-slick]').each(function(i,element){i=i+1;if(!$(element).hasClass('slick-initialized')){$(element).slick({dots:true,infinite:true,slidesToShow:1,slidesToScroll:1,prevArrow:'<div class="slick-prev"><i class="lq-icon-arrow-left"></i></div>',nextArrow:'<div class="slick-next"><i class="lq-icon-arrow-right"></i></div>',arrows:true,variableWidth:false,autoplay:true,autoplaySpeed:5000,responsive:[{breakpoint:MEDIA_MOBILE,settings:{dots:true,infinite:true,slidesToShow:1,slidesToScroll:1,centerMode:true,centerPadding:'6.66666%',arrows:false,autoplay:false,autoplaySpeed:4000}}]});}});home.resetBullet();},updateImage:function(){$('.lq-modalcoach__largepicture').each(function(i,item){i=i+1;if(MEDIA_TABLET>=$(window).width()){$(item).attr('src',$(item).attr('data-url-mobile'));}else{$(item).attr('src',$(item).attr('data-url'));}});},modalEvent:function(){$('.lq-coachs .lq-profile .lq-profile__btn').on('click',home.openModal);$('.lq-modalcoach .icon.lq-icon-close').on('click',home.closeModal);},openModal:function(e){e.preventDefault();var id=$(e.currentTarget).attr('data-modal'),slide=$('.lq-modalcoach[data-slide="'+id+'"]');home.topPosition=$(window).scrollTop();slide.fadeIn(450,function(){slide.addClass('lq-opened');$('header, footer, .lq-content').addClass('lq-hidden-block');$(window).scrollTop(0);});},closeModal:function(e){var currentSlide=$(this).closest('.lq-modalcoach');currentSlide.removeClass('lq-opened');$('header, footer, .lq-content').removeClass('lq-hidden-block');$(window).scrollTop(home.topPosition);setTimeout(function(){if($.browser.msie&&$.browser.version==8){currentSlide.hide();}else{currentSlide.fadeOut();}},100);},wrapCardText:function(){$('.lq-profile .lq-profile__btn').each(function(i,item){i=i+1;wrapLastWord(item);});},initial:function(){this.resetBullet();this.wrapCardText();this.slick();this.resizeEvent();this.setHeightCoach();this.updateImage();this.modalEvent();}};typing={cursor:'',speed:70,initialDelay:600,closeCursor:2000,initial:function(elem){var text=$.trim(elem.text()),arrtxt=text.split('');typing.wraping(arrtxt,elem);elem.css('visibility','visible').append('<i class="lq-typed-cursor"></i>');typing.cursor=elem.find('.lq-typed-cursor');setTimeout(function(){typing.animateTyping(elem,arrtxt);},typing.initialDelay);},wraping:function(arrayText,dest){var arrhtml=[];$(arrayText).each(function(i,e){i=i+1;arrhtml.push('<span>'+e+'</span>');});dest.html(arrhtml.join(''));},animateTyping:function(elem){var arrhtml=elem.find('span');arrhtml.each(function(i,e){setTimeout(function(){$(e).css({visibility:'visible'});typing.cursor.css({left:$(e).position().left+$(e).width()+1,top:$(e).position().top+$(e).height()*0.15});},i*typing.speed);});typing.hideCursor(arrhtml.length*typing.speed+typing.closeCursor);},hideCursor:function(delaytime){setTimeout(function(){typing.cursor.fadeOut(function(){typing.cursor.remove();});},delaytime);},getTime:function(elem){var arrhtml=elem.find('span');return(arrhtml.length*typing.speed)+typing.initialDelay;}};homehero={content:$('.lq-content-home'),footer:$('footer'),hero:$('.lq-homehero'),title:$('.lq-homehero__title'),menumovil:$('.lq-menusmartphone'),imgleft:$('.lq-homehero .lq-homehero__imgleft'),imgright:$('.lq-homehero .lq-homehero__imgright'),btn:$('.lq-homehero .lq-btn'),modal:$('.lq-homehero-modal'),btnclose:$('.lq-homehero-modal .lq-homehero-modal__head__close'),question1:$('.lq-homehero-modal__question--q1'),openedmodal:false,timecard:0,onResizeHero:function(){var wh=$(window).height()-$('header').height(),mh=0,factor=0.8;if(homehero.menumovil.is(':visible')){mh=homehero.menumovil.outerHeight(true);factor=1;}
homehero.hero.height((wh-mh)*factor);},onResizeWindow:function(){$(window).resize(function(){homehero.onResizeHero();});},showImages:function(){setTimeout(function(){homehero.imgleft.addClass('lq-show');setTimeout(function(){homehero.imgright.addClass('lq-show');},300);},300);},showButtons:function(delaytime){setTimeout(function(){homehero.hero.find('.lq-homehero__btns .lq-btn:eq(0)').addClass('lq-show');setTimeout(function(){homehero.hero.find('.lq-homehero__btns .lq-btn:eq(1)').addClass('lq-show');},150);},delaytime);},renderIE8:function(){if($.browser.msie&&$.browser.version==8){var data=homehero.hero.find('.lq-homehero__data');data.css('margin-top',-1*data.height()/2);}},onClickBtn:function(){homehero.btn.on('click',function(e){e.preventDefault();var btn=$(this),txt=btn.text(),bubble=homehero.question1.find('.lq_homehero-modal__btn > div'),key=btn.attr('data-key');if(!homehero.openedmodal){if(btn.hasClass('lq-redirect')){location.href=btn.attr('href');return false;}
homehero.hero.addClass('lq-preparepopup');bubble.text(txt);homehero.modal.find('.lq-slider').each(function(i,slid){if($(slid).hasClass('slick-initialized')){$(slid).slick('unslick');}});setTimeout(function(){homehero.openModal(key);},10);}});},openModal:function(key){var modal=$('.lq-homehero-modal[data-key='+key+']'),question1=modal.find('.lq-homehero-modal__question--q1');modal.fadeIn(400,function(){homehero.showModalElements(question1,1);});homehero.imgright.addClass('lq-hidelement');homehero.imgleft.addClass('lq-hidelement');setTimeout(function(){homehero.hero.find('.lq-homehero__data').addClass('lq-hidelement');},250);setTimeout(function(){homehero.content.addClass('lq-hidden-block');homehero.footer.addClass('lq-hidden-block');homehero.hero.removeClass('lq-preparepopup');homehero.hero.removeClass('lq-hidelement');homehero.btnclose.fadeIn();},500);homehero.openedmodal=true;},closeModal:function(){homehero.modal.find('.lq-homehero-modal__head__close').on('click',function(){var modal=$(this).closest('.lq-homehero-modal');homehero.hero.removeClass('lq-preparepopup');homehero.hero.find('.lq-homehero__data').removeClass('lq-hidelement');homehero.imgleft.removeClass('lq-hidelement');homehero.imgright.removeClass('lq-hidelement');homehero.content.removeClass('lq-hidden-block');homehero.footer.removeClass('lq-hidden-block');modal.fadeOut(400,function(){homehero.hideModalElements(modal);homehero.btnclose.fadeOut();});clearTimeout(homehero.timecard);homehero.openedmodal=false;});},showModalElements:function(questionContent,numquestion,keyslider){var title=questionContent.find('.lq-homehero-modal__title'),timeDelay=0,icon;questionContent.find('.lq_homehero-modal__btn > div').addClass('lq-show');setTimeout(function(){if($(window).width()<=MEDIA_MOBILE){icon=questionContent.find('.lq-chat-writing-img');icon.addClass('lq-show');setTimeout(function(){icon.removeClass('lq-show');setTimeout(function(){title.addClass('lq-show');homehero.showCardSlider(questionContent,numquestion,keyslider,350);},200);},1700);}else{title.addClass('lq-show');typing.initial(title);timeDelay=typing.getTime(title)+200;homehero.showCardSlider(questionContent,numquestion,keyslider,timeDelay);}},400);},hideModalElements:function(modal){var titles=modal.find('.lq-homehero-modal__title');modal.find('.lq_homehero-modal__btn > div').removeClass('lq-show');modal.find('.lq-homehero-modal__title').removeAttr('style').removeClass('lq-show');modal.find('.lq_homehero-modal__slider').removeClass('lq-show').removeAttr('style');modal.find('.lq-homehero-modal__question').removeClass('lq-hide').removeAttr('style');modal.find('.lq-homehero-modal__question--q2 .lq-slider').removeAttr('style');modal.find('.lq-chat-writing-img').removeClass('lq-show');titles.each(function(){$(this).text($(this).text());});homehero.modal.find('.lq-slider').each(function(i,slid){if($(slid).hasClass('slick-initialized')){$(slid).slick('unslick');}});},showCardSlider:function(questionContent,numquestion,keyslider,timeDelay){var contentslider=questionContent.find('.lq_homehero-modal__slider');homehero.timecard=setTimeout(function(){if(homehero.openedmodal){contentslider.show();homehero.setSlick(questionContent.find('.lq-slider'));if(numquestion==2){questionContent.find('.lq-slider[data-key='+keyslider+']').siblings().hide();}
contentslider.addClass('lq-show');}},timeDelay);},onClickCardSlider:function(){homehero.question1.find('.lq-slider__card').on('click',function(){var btn=$(this),key=btn.attr('data-key'),txt=btn.find('p').text(),modal=$(this).closest('.lq-homehero-modal'),question1=modal.find('.lq-homehero-modal__question--q1'),question2=modal.find('.lq-homehero-modal__question--q2');question1.addClass('lq-hide');question2.find('.lq_homehero-modal__btn > div').text(txt);setTimeout(function(){question1.hide();homehero.showModalElements(question2,2,key);},500);question2.show();});},setSlick:function(slider){if(!slider.hasClass('slick-initialized')){slider.slick({dots:true,infinite:false,slidesToShow:3,slidesToScroll:3,prevArrow:'<div class="slick-prev"><i class="lq-icon-arrow-left"></i></div>',nextArrow:'<div class="slick-next"><i class="lq-icon-arrow-right"></i></div>',responsive:[{breakpoint:MEDIA_MENU,settings:{slidesToShow:2,slidesToScroll:2,arrows:true}},{breakpoint:MEDIA_MOBILE,settings:{slidesToShow:1,slidesToScroll:1,centerMode:true,centerPadding:'6.6666%',arrows:false}}]});homehero.resetBullet(slider);slider.on('beforeChange',function(){homehero.resetBullet(slider);});}},resetBullet:function(slider){setTimeout(function(){slider.each(function(i,slid){$(slid).find('.slick-dots li').each(function(j,bullet){$(bullet).find('button').html('<span>'+(j+1)+'</span>'+' DE '+$(slid).find('.slick-dots li').length);});});},100);},showTitleMobile:function(){var img=homehero.hero.find('.lq-chat-writing-img'),mtitle=homehero.hero.find('.lq-homehero__title--addmobile');img.fadeIn();setTimeout(function(){img.fadeOut(200,function(){mtitle.addClass('lq-show');img.css('top',homehero.title.position().top+5);setTimeout(function(){img.fadeIn();},300);setTimeout(function(){img.fadeOut(200,function(){homehero.title.addClass('lq-show');});homehero.showButtons(700);},2500);});},2000);},initial:function(){this.onResizeHero();this.onResizeWindow();this.showImages();this.renderIE8();this.onClickBtn();this.closeModal();this.onClickCardSlider();}};$(document).ready(function(){homehero.initial();home.initial();setTimeout(function(){if($(window).width()>MEDIA_MOBILE){typing.initial(homehero.title);homehero.showButtons(typing.getTime(homehero.title)+200);homehero.title.addClass('lq-show');homehero.hero.find('.lq-homehero__title--addmobile').addClass('lq-show');}else{homehero.showTitleMobile();}},500);$(".simple-content .lq-modalcoach").each(function(index,item){var listCategoria=$(this).find(".lq-modalcoach__menu .list");if($(listCategoria).length>0){var categorias_menu=$(this).find(".lp-coach-categoria");$(categorias_menu).each(function(i,element){$(listCategoria).append($(this).html());});$(categorias_menu).remove();}});$("#open").click();});$('#open').on('click',function(){var flag=$('#imgFlagId').val();if(flag=="1"){$('#popup').fadeIn('slow');$('.popup-overlay').fadeIn('slow');var imgPo=$('#imgPopupId').val();var linkDet=$('#linkPopupId').val();$("#imgCampana").attr("src",imgPo);$("#linkDetalle").attr("href",linkDet);$("#linkDetalle").attr("target","_blank");return false;}});$('#close').on('click',function(){$('#popup').fadeOut('slow');$('.popup-overlay').fadeOut('slow');return false;});$('#transparenteId1').live("click",function(){$('#popup').fadeOut('slow');$('.popup-overlay').fadeOut('slow');return false;});}());